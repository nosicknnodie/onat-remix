views:
  - type: table
    name: 모든 작업
    filters:
      and:
        - file.folder == "tasks"
    order:
      - file.name
      - title
      - status
      - priority
      - category
    sort:
      - property: priority
        direction: ASC
    fields:
      - title
      - status
      - priority
      - category
      - created_date
    columnSize:
      note.title: 240
